class Nepoogle(QWidget):
    cache = []
    cacheViewer = []
    currUri = None
    clearResourceManagerCache = True
    keyModifiers = Qt.NoModifier
    model = None
    navigationData = []
    pendingQuery = False
    queriesIndex = -1
    queryMethod = None
    renderedCache = ''
    renderedRows = 0
    renderSize = 50
    resultData = []
    resultStructure = []
    resultTime = None
    sparql = None
    textToFind = ""
    verboseMode = False
    warningsList = []

    iconDelete = KIconLoader().iconPath('edit-delete', KIconLoader.NoGroup)
    iconDocumentInfo = KIconLoader().iconPath('documentinfo', KIconLoader.NoGroup)
